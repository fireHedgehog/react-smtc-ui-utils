{
  "name": "react-smtc-ui-utils",
  "version": "1.4.5",
  "description": "react-smtc-ui-utils React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "homepage": "https://firehedgehog.github.io/react-smtc-ui-utils/#/",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d demo/dist",
    "docgen": "react-docgen src --pretty",
    "docgen-resolver": "react-docgen src --pretty --resolver findAllComponentDefinitions -o src/doc/doc.json"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-resize-detector": "^6.7.6"
  },
  "devDependencies": {
    "npm-check-updates": "^11.8.5",
    "gh-pages": "^3.2.3",
    "highlight.js": "^11.2.0",
    "html-entities": "^2.3.2",
    "nwb": "^0.25.2",
    "react": "^17.0.2",
    "react-docgen": "^5.4.0",
    "react-dom": "^17.0.2",
    "react-highlight": "^0.14.0",
    "react-router-dom": "^5.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4"
  },
  "author": "rockychen",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component",
